<Window x:Class="SQL_lite_database_search_wpf.UI.Team.AddUserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SQL_lite_database_search_wpf.UI.Team"
        mc:Ignorable="d"
        Title="AddUserWindow" Height="82" Width="264">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        
        <StackPanel Orientation="Horizontal" Grid.Row="0">
            <TextBlock Text="User Name :"/>
            <TextBlock x:Name="UITbUserName" MinWidth="60"/>
        </StackPanel>
        <Button x:Name="UIBtOK" Grid.Row="1" Click="UIBtOK_Click"/>

    </Grid>
</Window>
