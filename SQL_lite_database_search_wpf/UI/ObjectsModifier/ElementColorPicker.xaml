<mui:ModernWindow x:Class="SQL_lite_database_search_wpf.UI.ObjectsModifier.ElementColorPicker"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SQL_lite_database_search_wpf.UI.ObjectsModifier"
      
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="ElementColorPicker">

    <Grid>
        
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>


        <ListBox x:Name="UIListColors" Grid.Row="0">
           
        </ListBox>


        <StackPanel Grid.Row="1" Orientation="Horizontal"           >
            <Button Content="OK"/>
            <Button Content="Cancel"/>
        </StackPanel>
    </Grid>
</mui:ModernWindow>
